                script {
                    echo 'Testing..'
                    /*sh './gradlew test'*/
                }
            }
        }
        stage('Build') {
            steps {
                echo 'Building..'
                bat './gradlew bootJar'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}